config opennds
	option enabled '1'
	option gatewayinterface 'br-lan'
	option gatewayname 'Prepper Pi'
	option maxclients '250'
	option preauthidletimeout '30'
	option authidletimeout '120'
	option sessiontimeout '1200'
	option checkinterval '600'
	option preauth 'none'
	option binauth '/usr/lib/opennds/binauth_log.sh'
	option redirectURL 'http://10.20.30.1/'
	option splashpage '/etc/opennds/htdocs/splash.html'
	option statuspage '/etc/opennds/htdocs/status.html'
	option webroot '/etc/opennds/htdocs/'
	option blocklist '/etc/opennds/blocklist'
	option allowlist '/etc/opennds/allowlist'
	option trustedmac '/etc/opennds/trustedmac'
	option fwhook_enabled '1'
	list users_to_router 'allow udp port 67'
	list users_to_router 'allow udp port 53'
	list users_to_router 'allow tcp port 53'
	list users_to_router 'allow tcp port 80'
	list users_to_router 'allow tcp port 443'
	list authenticated_users 'allow all'
	list preauthenticated_users 'allow tcp port 53'
	list preauthenticated_users 'allow udp port 53'
	list preauthenticated_users 'allow udp port 67'
	list preauthenticated_users 'allow tcp port 80'
	list preauthenticated_users 'allow tcp port 443'
